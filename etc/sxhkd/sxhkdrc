super + Return
	xterm

super + shift + e
	killall Xorg

super + p
	ndmen

super + shift + s
	slock

super + shift + q
	bspc node -k

super + {t,m}
	bspc desktop -l {tiled,monocle}

super + f
	bspc node -t "~floating"

super + {_,shift + }{1,2,3,4,5,6,7,8,9,0}
	bspc {desktop -f,node -d} '^{I,II,III,IV,V,VI,VII,VIII,IX,X}'

super + {j,k}
	bspc node -f {next,prev}
